<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.coordinatorlayout.widget.CoordinatorLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        tools:context=".ui.privacypolicy.ActivityPrivacyPolicy">
        <com.google.android.material.appbar.AppBarLayout
            android:id="@+id/app_bar_layout_privacy_policy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:theme="@style/Theme.ProjectRestaurant.AppBarOverlay">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar_privacy_policy"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                app:navigationIcon="?homeAsUpIndicator"
                app:popupTheme="@style/Theme.ProjectRestaurant.PopupOverlay"/>
        </com.google.android.material.appbar.AppBarLayout>
        <include layout="@layout/content_privacy_policy"/>
    </androidx.coordinatorlayout.widget.CoordinatorLayout>
</layout>